package com.dsa;

public class SmallestInArr {

	public static void main(String[] args) {
		
		int[] arr = { 12,4,3,13,34,11,45,8};
		
		int temp;
		
		for(int  i = 0; i < arr.length; i++)
		{
			for(int j = i + 1; j < arr.length; j++)
			{
				if(arr[i] > arr[j])				
				{
					temp = arr[i];
					arr[i] = arr[j];
					arr[j] = temp;
				}
			}
		}
		System.out.println("Smallest element in array is : "+arr[1]);
	}

}
