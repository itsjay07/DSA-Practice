package com.dsa;

public class LargestEleInArray {

	public static void main(String[] args) {

		int arr[] = new  int [] {35,54,23,65,7,4,22};
		
		int max = arr[0];
		
		for(int i = 1; i < arr.length;i++)
		{
			if(arr[i] > max)
			{
				max = arr[i];
			}
		}
		
		System.out.println("Largest element of an array is : "+max);
	}

}
