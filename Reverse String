package com.jay;

public class ReverseString {

	public static void main(String[] args) {
		
		String str = "deep";
		int n = str.length(); 
		String rev ="";
		
		System.out.println("Original String : "+str);
		for(int i = n - 1; i >=0; i--)
		{
			rev = rev + str.charAt(i);
		}
		
		System.out.println("reverse String : " +rev);

	}

}
