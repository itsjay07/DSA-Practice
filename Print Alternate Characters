package com.dsa;

public class PrintAlternateCharacters {

	public static void main(String[] args) {

		String str1 = "jyat";
		String str2 = "awn";

		int i = 0;
		int j = 0;

		while (i < str1.length() || j < str2.length()) 
		
		{
			if(i < str1.length())
			{
				System.out.print(str1.charAt(i)); 
				i++;
			}
			
			if(j < str2.length())
			{
			  System.out.print( str2.charAt(j));
				j++;
			}

		}

	}

}
