package com.dsa;

public class CheckPalindromOrNot {
	
	public static void main(String[] args) {
		
		String str  = "07070";
		String rev = "";
		
		for(int i = str.length()-1; i  >= 0; i--)
		{
			rev = rev + str.charAt(i);
		}
		
		if(str.equals(rev))
		{
			System.out.println("Yes its Palindrom");
		}
		else
		{
			System.out.println("Not a Palindrom");
		}
		
	}

}
