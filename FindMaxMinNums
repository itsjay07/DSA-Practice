package com.dsa;

public class FindMaxMinNums 
{
	public static void main(String[] args) {
		
		int[]arr = {2,4,1,7,9};
		
		int max = arr[0];
		int min = arr[0];
		
		for(int i = 0; i < arr.length; i++)
		{
			if(arr[i] > max)
			{
				max = arr[i];
			}
			
			if(arr[i] < min)
			{
				min = arr[i];
			}
		}
		
		System.out.println("Maximum Number is :"+max);
		System.out.println("Minimum Number is :"+min);
	}
}
